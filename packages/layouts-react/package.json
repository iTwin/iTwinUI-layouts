{
  "name": "@itwin/itwinui-layouts-react",
  "version": "0.1.0",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "description": "iTwinUI package that provides React components for most common layouts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "license": "MIT",
  "homepage": "https://github.com/iTwin/iTwinUI-layouts",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/iTwinUI-layouts.git",
    "directory": "packages/layouts-react"
  },
  "keywords": [
    "layouts",
    "css",
    "sass",
    "scss",
    "html",
    "ui",
    "ux",
    "standards",
    "bentley systems",
    "itwin",
    "itwinui",
    "react-component",
    "react"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build:watch": "chokidar \"src/**/*\" -c \"yarn build\""
  },
  "dependencies": {
    "@itwin/itwinui-layouts-css": "workspace:*",
    "classnames": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0",
    "react-dom": "^16.8.6 || ^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "chokidar-cli": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.4"
  }
}
